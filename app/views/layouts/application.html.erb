<!DOCTYPE html>
<html>
<head>
  <title>TennisReserve</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="pull-right">
	    <% if user_signed_in? %>
	     <%= link_to "Edit Profile", edit_user_registration_path, class: "btn btn-info" %> 
	     <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "btn btn-danger" %>
	    <% end %>  
	 </div>


<div class="container">
	<%= yield %>
</div>

</body>
</html>
